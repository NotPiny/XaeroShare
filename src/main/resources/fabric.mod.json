{
  "schemaVersion": 1,
  "id": "xaeroshare",
  "version": "${version}",
  "name": "XaeroShare",
  "description": "Adds the ability to share xaero's minimap waypoints with individual players",
  "authors": [
    "Piny"
  ],
  "contact": {
    "issues": "https://github.com/NotPiny/XaeroShare/issues",
    "sources": "https://github.com/NotPiny/XaeroShare",
    "homepage": "https://modrinth.com/mod/xaeroshare"
  },
  "license": "MIT",
  "icon": "assets/xaeroshare/icon.png",
  "environment": "client",
  "entrypoints": {
    "fabric-datagen": [
      "dev.piny.xaeroshare.client.XaeroshareDataGenerator"
    ],
    "client": [
      "dev.piny.xaeroshare.client.XaeroshareClient"
    ],
    "main": [
      "dev.piny.xaeroshare.Xaeroshare"
    ]
  },
  "mixins": [
    "xaeroshare.mixins.json",
    {
      "config": "xaeroshare.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": ">=${minecraft_version}"
  }
}
